<?xml version="1.0"?>
<object name="portal_skins" allow_any="False" cookie_persistence="False"
   default_skin="pretaweb.pretamap">

 <object name="pretaweb_pretamap_custom_images"
    meta_type="Filesystem Directory View"
    directory="pretaweb.pretamap:skins/pretaweb_pretamap_custom_images"/>
 <object name="pretaweb_pretamap_custom_templates"
    meta_type="Filesystem Directory View"
    directory="pretaweb.pretamap:skins/pretaweb_pretamap_custom_templates"/>
 <object name="pretaweb_pretamap_styles"
    meta_type="Filesystem Directory View"
    directory="pretaweb.pretamap:skins/pretaweb_pretamap_styles"/>

 <skin-path name="pretaweb.pretamap" based-on="Plone Default">
  <layer name="pretaweb_pretamap_custom_images"
     insert-after="custom"/>
  <layer name="pretaweb_pretamap_custom_templates"
     insert-after="pretaweb_pretamap_custom_images"/>
  <layer name="pretaweb_pretamap_styles"
     insert-after="pretaweb_pretamap_custom_templates"/>
 </skin-path>

</object>
